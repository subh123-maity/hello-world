import numpy as np
matrix=[]
rows=int(input("enter the rows:- "))
columns=int(input("enter the number of columns:- "))
for i in range(rows):
    a=[]
    for j in range(columns):
        val=int(input("enter the numbers:- "))
        a.append(val)
    matrix.append(a)
arr=np.array(matrix)
for i in range(rows):
    for j in range(columns):
        print(arr[i][j],end=' ')
    print( )
