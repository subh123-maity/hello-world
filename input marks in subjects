p=int(input("enter the marks in physics: "))
c=int(input("enter the marks in chemistry: "))
m=int(input("enter the marks in maths: "))
b=int(input("enter the marks in biology: "))
s=((p+c+m+b)/400)*100
if(s>=90):
  print("GRADE A")
elif(s>=80):
  print("GRADE B")
elif(s>=70):
  print("GRADE C") 
elif(s>=60):
  print("GRADE D") 
elif(s>=40):
  print("GRADE E") 
elif(s<40):
  print("GRADE F") 
 
