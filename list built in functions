lst1=[1,2,3,4,5]
lst2=(7,8,9,10)                #list(seq) converts a seq to list
print(list(lst2))
lst1.append(1)                   #append (adds   in the end
print(lst1)
lst1.remove(1)                            #removes that element from the list
print(lst1)
lst1.pop(3)                       #pop(index)   removes the number on that index
print(lst1)
lst1.reverse()                 #reverses the elements from the list
print(lst1)
lst1.sort()                       #sorts the elements in the list
print(lst1)
print(lst1.count(1))                            #counts the repetiotion of the elements in the list
idx=lst1.index(3)                                #prints the index of the element in the list
print(idx)
lst1.extend(lst2)                                              #extend can be used to concatenate the elments to a list
print(lst1)
lst1.insert(0,78)                           #can insert an element anywhere and on any index in the list
print(lst1)
import random                                  #random module shuffle in a library
random.shuffle(lst1)
print(lst1)
